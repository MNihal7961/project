<%- include('../partials/user-head.ejs') %>
  <%- include('../partials/user-nav.ejs') %>

    <style>
      .btn-color {
        background-color: #014122;
        color: #fff;

      }

      .profile-image-pic {
        height: 200px;
        width: 200px;
        object-fit: cover;
      }

      .cardbody-color {
        background-color: #eee;
      }

      a {
        text-decoration: none;
      }
    </style>
    <div class="container">
      <div class="row">
        <div class="col-md-6 offset-md-3">
          <h2 class="text-center text-dark mt-5">Add Address</h2>
          <div class="text-center mb-5 text-dark">For Faster Delivery</div>
          <div class="card my-5">

            <form class="card-body cardbody-color p-lg-5" method="post" action="/user/add/address">

              <div class="text-center">
                <img
                  src="data:image/png;base64,"
                  class="img-fluid profile-image-pic img-thumbnail rounded-circle my-3" width="200px" alt="profile">
              </div>

              <div class="mb-3">
                <input required type="text" class="form-control" placeholder="First Name" name="firstname">
              </div>

              <div class="mb-3">
                <input required type="text" class="form-control" placeholder="Last Name" name="lastname">
              </div>

              <div class="mb-3">
                <input required type="text" class="form-control" placeholder="Street" name="street">
              </div>

              <div class="mb-3">
                <input required type="text" class="form-control" placeholder="Building Name" name="building">
              </div>

              <div class="mb-3">
                <input required type="text" class="form-control" placeholder="City" name="city">
              </div>

              <div class="mb-3">
                <input required type="text" class="form-control" placeholder="State" name="state">
              </div>

              <div class="mb-3">
                <input required type="number" class="form-control" placeholder="Pincode" name="pincode">
              </div>

              <div class="mb-3">
                <input required type="number" class="form-control" placeholder="Phone Number" name="mobile">
              </div>

              <div class="mb-3">
                <input required type="email" class="form-control" placeholder="Email Address" name="email">
              </div>

              <div class="text-center">
                <button type="submit" class="btn btn-color px-5 mb-5 w-100"
                  onclick="return confirm('Are you sure you want to add new address?')">Add To Address</button>
              </div>

              <a href="/user/address/book" class="float-end btn btn-primary">Back to Address Book</a>
            </form>
          </div>

        </div>
      </div>
    </div>

    <%- include('../partials/user-foot.ejs') %>